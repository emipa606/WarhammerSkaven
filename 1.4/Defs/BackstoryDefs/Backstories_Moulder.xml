<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.AlienBackstoryDef>
    <defName>SkavenMasterMoulder</defName>
    <title>Master moulder</title>
    <titleShort>Master moulder</titleShort>
    <baseDesc>[PAWN_nameDef] is at the peak of the clan moulders. A true master at [PAWN_possessive] vile and gruesome work.</baseDesc>
    <bodyTypeMale>Fat</bodyTypeMale>
    <bodyTypeFemale>Fat</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <key>Intellectual</key>
        <value>4</value>
      </li>
      <li>
        <key>Crafting</key>
        <value>3</value>
      </li>
      <li>
        <key>Animals</key>
        <value>1</value>
      </li>
      <li>
        <key>Medicine</key>
        <value>6</value>
      </li>
    </skillGains>
    <forcedTraitsChance>
      <li>
        <defName>Psychopath</defName>
        <chance>90</chance>
      </li>
      <li>
        <defName>Industriousness</defName>
        <degree>2</degree>
        <chance>50</chance>
      </li>
    </forcedTraitsChance>
    <forcedHediffs>
      <li>SkavenMoulderWarpSpine</li>
    </forcedHediffs>
    <spawnCategories>
      <li>SkavenMoulder</li>
    </spawnCategories>
    <requiredWorkTags>
      <li>Intellectual</li>
      <li>Violent</li>
    </requiredWorkTags>
  </AlienRace.AlienBackstoryDef>
